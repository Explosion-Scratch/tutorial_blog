<script type="module">
		import * as _$ from "https://cdnjs.cloudflare.com/ajax/libs/Bijou.js/7.4.0/bijou-min.js";
		_$.each(document.querySelectorAll(".button"), (btn) => {
			btn.setAttribute("data-ripple", "");
			btn.setAttribute("data-event", "mousedown")
			btn.setAttribute("data-time", 300);
			btn.setAttribute("data-color", "lightseagreen");
			btn.setAttribute("data-opacity", ".5");
		})
		ripple();
